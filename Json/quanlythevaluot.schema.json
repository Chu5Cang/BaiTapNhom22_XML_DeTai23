{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "title": "HeThongBaiGiuXe",
  "type": "object",
  "properties": {
    "thexe": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "mathe": {"type": "integer","minimum": 1},
          "trangthai": {"type": "boolean"}
        },
        "required": ["mathe", "trangthai"],
        "additionalProperties": false
      }
    },
    "luotgiuxe": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "maluot": { "type": "string", "pattern": "^LG\\d{2}\\d{6}$" },
          "biensoRef": { "type": "string" },
          "matheRef": {"type": "integer","minimum": 1},
          "mapcRef": { "type": "string" },
          "thoigianvao": { "type": "string", "format": "date-time" },
          "thoigianra": { "type": "string", "format": "date-time" },
          "tongthoigian": { "type": "integer", "minimum": 0 },
          "tongtien": { "type": "number", "exclusiveMinimum": 0 }
        },
        "required": ["maluot","biensoRef","matheRef","mapcRef","thoigianvao","thoigianra","tongthoigian","tongtien"],
        "additionalProperties": false
      }
    }
  },
  "required": ["thexe", "luotgiuxe"],
  "additionalProperties": false
}
